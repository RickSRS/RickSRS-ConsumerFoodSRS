@model HomeViewModel

<div class="container-fluid">
    <partial name="_Carousel" />
</div>

<div class="pt-3 container">
    <div class="card">
        <h3 class="card-header">
            Os preferidos da semana
        </h3>
        <div class="card-body">
            <div class="container">
                <div class="row">
                    @foreach (var lanche in Model.LanchesPreferidos)
                    {
                        <partial name="_LanchesResumo" for="@lanche" />
                    }
                </div>
            </div>
        </div>
    </div>
</div>